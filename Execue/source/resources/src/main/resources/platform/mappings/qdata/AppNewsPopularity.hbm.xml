<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.execue.core.common.bean.qdata.AppNewsPopularity" table="APP_NEWS_POPULARITY">
        
        <id name="id" type="java.lang.Long">
            <column name="ID" precision="20" scale="0" />
            <generator class="org.hibernate.id.enhanced.TableGenerator">
                <param name="table_name">SEQUENCES</param>
                <param name="segment_value">APP_NEWS_POPULARITY</param>
            </generator>
        </id>
        
        <property name="udxId" type="java.lang.Long">
            <column name="UDX_ID" precision="20" scale="0" />
        </property>
        
        <property name="entityBeId" type="java.lang.Long">
            <column name="ENTITY_BE_ID" precision="20" scale="0" />
        </property>
        
        <property name="appId" type="java.lang.Long">
            <column name="APP_ID" precision="20" scale="0" />
        </property>
        
        <property name="hits" type="java.lang.Integer">
            <column name="HITS" precision="10" scale="0" />
        </property>
    </class>

</hibernate-mapping>
