<?xml version="1.0" encoding="ISO-8859-1" ?>
<execue-configuration>
	<nlp>
		<files>
			<taggerDataFile>/platform/bean-config/kbase/tagger-dictionary/wsj3t0-18.holder</taggerDataFile>
			<execueTagsFile>/platform/bean-config/kbase/execueTagger.xml</execueTagsFile>
			<languagePatternsFile>/platform/bean-config/kbase/languagePatternRecognitionRules.xml</languagePatternsFile>
			<languageAssociationPatternsFile>/platform/bean-config/kbase/languageAssociationPatternRules.xml</languageAssociationPatternsFile>
		</files>
		<flags>
		    <taggerDataFileLocationFlag>EMBEDED</taggerDataFileLocationFlag> <!-- FILE_SYSTEM|EMBEDED -->
			<snowflakePercentageThreshold>65</snowflakePercentageThreshold>
			<snowflakePercentileThreshold>70</snowflakePercentileThreshold>
			<numberSnowflakesSelected>1</numberSnowflakesSelected>
			<stringMatchingThreshold>0.75</stringMatchingThreshold>
			<outOfOrderWeightReductionPercent>10.0</outOfOrderWeightReductionPercent>
			<sflProximityReductionPercent>5.0</sflProximityReductionPercent> 
			<unAssociatedEntityReductionPercent>20.0</unAssociatedEntityReductionPercent> 
			<indirectAssociationReductionPercent>5.0</indirectAssociationReductionPercent>
			<implicitAssociationReductionPercent>10.0</implicitAssociationReductionPercent>     
		</flags>
		<contexts>
			<context>
				<name>Default</name>
				<Default>
					<semanticScoping>
						<processors>
							<processorClassName>semanticScopingPWProcessor</processorClassName>
							<processorClassName>semanticScopingSnowFlakeProcessor</processorClassName>
						</processors>
					</semanticScoping>
				</Default>
			</context>
			<context>
				<name>TypeCloud</name>
				<TypeCloud>
					<processors>
						<processorClassName>meaningFinder</processorClassName>
					</processors>
				</TypeCloud>
			</context>
			<context>
				<name>AppCloud</name>
				<AppCloud>
					<processors>
						<processorClassName>meaningEnhancer</processorClassName>
					</processors>
				</AppCloud>
			</context>	
		</contexts>
		<preProcessors>
			<preProcessorClassName>com.execue.nlp.preprocessor.QueryNormalizer</preProcessorClassName>
			<preProcessorClassName>com.execue.nlp.preprocessor.ExeCueTagger</preProcessorClassName>
			<query-normalizer>
				<currency-pattern-regex>
					<![CDATA[([<]|[>]|[=]|[<=]|[>=])?([$]?)(([\\d]+([\,][\\d]+)*)([\\.]\\d+)?|([\\d]+([\,][\\d]+)*)?([\\.]\\d+))([MmKkBbTt]?)([$]?)|([MmKkBbTt])([$])]]>
				</currency-pattern-regex>
				<area-pattern-regex>
					<![CDATA[([<]|[>]|[=]|[<=]|[>=])?([\\d]+([\,][\\d]+)*([\\.]\\d+)?)([Ss][Qq][Ff][Tt])]]>
				</area-pattern-regex>
			</query-normalizer>
		</preProcessors>
		<wordTypeWeights>
            <wordType>
                <name>Exact</name>
                <value>1</value>
                <weight>100</weight>
            </wordType>
            <wordType>
                <name>DisplayName</name>
                <value>2</value>
                <weight>95</weight>
            </wordType>
            <wordType>
                <name>Synonym</name>
                <value>3</value>
                <weight>85</weight>
            </wordType>
            <wordType>
                <name>Abbreviation</name>
                <value>4</value>
                <weight>75</weight>
            </wordType>
            <wordType>
                <name>Acronym</name>
                <value>5</value>
                <weight>70</weight>
            </wordType>
            <wordType>
                <name>Code</name>
                <value>6</value>
                <weight>65</weight>
            </wordType>
            <wordType>
                <name>Tag</name>
                <value>7</value>
                <weight>60</weight>
            </wordType>
            <wordType>
                <name>Description</name>
                <value>8</value>
                <weight>50</weight>
            </wordType>
            <wordType>
                <name>RelatedWord</name>
                <value>9</value>
                <weight>50</weight>
            </wordType>
            <wordType>
                <name>LinguisticRoot</name>
                <value>10</value>
                <weight>50</weight>
            </wordType>
            <wordType>
                <name>EntityVariant</name>
                <value>11</value>
                <weight>95</weight>
            </wordType>
            
        </wordTypeWeights>
        <appSpecificMethods>
        	<appSpecificMethod>
        		<methodName>reviseReducedFormByPositions</methodName>
        		<appName>Craigslist Autos</appName>
        	</appSpecificMethod>
        </appSpecificMethods>
		<static-values>
			<max-tokens-limit>40</max-tokens-limit>
			<nlp-time-based-cutoff-enabled>true</nlp-time-based-cutoff-enabled>
			<max-allowed-nlp-time>20000</max-allowed-nlp-time> <!-- no longer being used, as we have below section level max limits defined -->
        	<max-allowed-nlp-time-for-semantic-scoping>10000</max-allowed-nlp-time-for-semantic-scoping>
			<max-allowed-nlp-time-for-finding-semantics>10000</max-allowed-nlp-time-for-finding-semantics>
			<max-allowed-nlp-time-for-enhancing-semantics>10000</max-allowed-nlp-time-for-enhancing-semantics>
			<max-allowed-pw-words>10</max-allowed-pw-words>
        	<max-allowed-possibility>5</max-allowed-possibility>
        	<choose-best-possibility>true</choose-best-possibility>
        	<add-linguistic-root>false</add-linguistic-root>
        	<filter-onto-recognitions>true</filter-onto-recognitions>
        	<!-- max allowed best onto instance recognition per concept/time for same token position -->
        	<max-allowed-best-instance-recognitions-for-token>2</max-allowed-best-instance-recognitions-for-token>
        	<filter-possibility-with-hanging-relations>false</filter-possibility-with-hanging-relations>
			<penalty-for-missing-part-in-name>0</penalty-for-missing-part-in-name>
			<unification_framework_cloud_id>500</unification_framework_cloud_id>
			<penalize-for-implicit-concept-association-by-application>
				<penalize-for-implicit-concept-association>
					<name>default</name>
					<value>true</value>
				</penalize-for-implicit-concept-association>
				<penalize-for-implicit-concept-association>
					<name>Craigslist Autos</name>
					<value>false</value>
				</penalize-for-implicit-concept-association>
			</penalize-for-implicit-concept-association-by-application>
			<maximum-proximity-for-sfl>10</maximum-proximity-for-sfl>
			<path_quality_threshold>0.5</path_quality_threshold>
		</static-values>
		<allowed-unconnected-types>
			<name>Adjective</name>
			<name>Conjunction</name>
			<name>Preposition</name>
			<name>ByConjunction</name>
			<name>SubordinatingConjunction</name>
			<name>CoordinatingConjunction</name>
			<name>RangePreposition</name>
			<name>ValuePreposition</name>
			<name>Punctuation</name>
		</allowed-unconnected-types>
		<default-rf-recognition-weights>
			<concept-weight>10</concept-weight>
			<instance-weight>11</instance-weight>
			<relation-weight>10</relation-weight>
		</default-rf-recognition-weights>
	</nlp>
</execue-configuration>