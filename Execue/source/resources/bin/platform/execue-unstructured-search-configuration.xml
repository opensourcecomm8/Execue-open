<?xml version="1.0" encoding="ISO-8859-1" ?>
<execue-configuration>
	<unstructured-search>
		<static-values>
			<defaultUserPreferredZipCode>75214</defaultUserPreferredZipCode>
			<defaultVicinityDistanceLimit>100</defaultVicinityDistanceLimit>
			<defaultVicinityDistanceLimits>100\,500\,1000\,2000\,5000\,10000</defaultVicinityDistanceLimits>
			<displayCloseMatchCount>false</displayCloseMatchCount>
			<performCompleteKeyWordBasedSearch>true</performCompleteKeyWordBasedSearch>
			<special-char-trim-pattern-for-title>[\?\*\$\~\%\@\^\_\|\#\;\.]{2\,}</special-char-trim-pattern-for-title>		  
			<unstructured-app-user-visibility>true</unstructured-app-user-visibility>
			<apply-key-word-search-filter>true</apply-key-word-search-filter>
			<apply-partial-match-filter>false</apply-partial-match-filter>
			<numberOfLinks>10</numberOfLinks>
			<pageSize>20</pageSize>
			<defaultSemantifiedContentKeywordMatchMaxRecordCount>500</defaultSemantifiedContentKeywordMatchMaxRecordCount>
			<resemantificationSemantifiedContentKeywordMatchMaxRecordCount>10</resemantificationSemantifiedContentKeywordMatchMaxRecordCount>
			<resemantification-checkbox-visibility>true</resemantification-checkbox-visibility>
			<suggestion-retrieval-limit>10</suggestion-retrieval-limit>
			<use-db-function-for-multiple-location-query>false</use-db-function-for-multiple-location-query>
			<db-function-name-for-multiple-location>MIN_ST_LN_DIST</db-function-name-for-multiple-location>
		</static-values>
		<SearchResultOrder>
			<pre-order>
				<ResultOrder>
					<name>SEMANTIC_SCORE</name>
					<priority>1</priority>
					<type>DESC</type>
				</ResultOrder>
				<ResultOrder>
					<name>MATCHED_FACTOR</name>
					<priority>2</priority>
					<type>DESC</type>
				</ResultOrder>
			</pre-order>
			<distance-order>
				<ResultOrder>
					<name>DISTANCE</name>
					<priority>3</priority>
					<type>ASC</type>
				</ResultOrder>
			</distance-order>
			<post-order>
				<ResultOrder>
					<name>KEY_WORD_MATCH_SCORE</name>
					<priority>4</priority>
					<type>DESC</type>
				</ResultOrder>
				<ResultOrder>
					<name>SEMANTIFIED_CONTENT_DATE</name>
					<priority>5</priority>
					<type>DESC</type>
				</ResultOrder>
			</post-order>
		</SearchResultOrder>
		<resultHeaders>
			<resultHeader>
				<id>YEAR</id>
				<name>Model Year</name>
				<format></format>
			</resultHeader>
			<resultHeader>
				<id>PRICE</id>
				<name>Price</name>
				<format>$</format>
			</resultHeader>
			<resultHeader>
				<id>MILEAGE</id>
				<name>Mileage</name>
				<format></format>
			</resultHeader>
			<resultHeader>
				<id>DATE</id>
				<name>Posted Date</name>
				<format></format>
			</resultHeader>
			<resultHeader>
				<id>DISTANCE</id>
				<name>Distance</name>
				<format></format>
			</resultHeader>
			<resultHeader>
				<id>MATCH</id>
				<name>Match</name>
				<format></format>
			</resultHeader>
		</resultHeaders>
		<unstructuredSearchResultHeaders>			
			<resultHeader>
				<id>DATE</id>
				<name>Posted Date</name>
				<format></format>
				<columnName>semantified_content_date</columnName>
				<featureValueType>DT</featureValueType>
				<sortable>Y</sortable>
				<defaultSortOrder>DESC</defaultSortOrder>
				<diplayableFeatureAlignmentType>left</diplayableFeatureAlignmentType>
				<dataHeader>N</dataHeader>	
			</resultHeader>			
			<resultHeader>
				<id>MATCH</id>
				<name>Match</name>
				<format></format>
				<columnName>SEMANTIC_SCORE</columnName>
				<featureValueType>N</featureValueType>
				<sortable>N</sortable>
				<defaultSortOrder>DESC</defaultSortOrder>
				<diplayableFeatureAlignmentType>right</diplayableFeatureAlignmentType>
				<dataHeader>N</dataHeader>	
			</resultHeader>
		</unstructuredSearchResultHeaders>	
							
	</unstructured-search>
</execue-configuration>