<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.execue.core.common.bean.sdata.location.CountryState" table="COUNTRY_STATE">
		<id name="id" type="java.lang.Long">
			<column name="ID" precision="20" scale="0" />
			<generator class="org.hibernate.id.enhanced.TableGenerator">
                <param name="table_name">SEQUENCES</param>
                <param name="segment_value">COUNTRY_STATE</param>
            </generator>
		</id>
		<property name="countryId" type="java.lang.Long">
			<column name="COUNTRY_ID" precision="20" scale="0" />
		</property>
		<property name="stateId" type="java.lang.Long">
			<column name="STATE_ID" precision="20" scale="0" />
		</property>
	</class>
</hibernate-mapping>
